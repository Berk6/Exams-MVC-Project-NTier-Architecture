
@model List<ExamResultViewModel>
@if(ViewBag.NotFound==null){

    <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Test Adı</th>
      <th scope="col">Test Sahibi</th>
      <th scope="col">Sonuç</th>
    </tr>
  </thead>
  <tbody>

    @for (int i = 0; i < Model.Count; i++)
    {
    <tr>
      <th scope="row">@(i+1)</th>
      <td>@Model[i].Exam.Name</td>
      <td>@Model[i].Exam.Creater.UserName</td>
                    <td>@Model[i].Result</td>
  </tr>
    }
  </tbody>
</table>
}
else{
    <div class="dikkat">@ViewBag.NotFound</div>
}